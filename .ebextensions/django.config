option_settings:
  aws:elasticbeanstalk:container:python:
    WSGIPath: LetsTourTec/wsgi.py
  aws:elasticbeanstalk:container:python:staticfiles:
    /static/: static
    /media/: media/

container_commands:
  01_collectstatic:
    command: "source /var/app/current/github.io3/venv/Scripts/activate && python manage.py collectstatic --noinput"
